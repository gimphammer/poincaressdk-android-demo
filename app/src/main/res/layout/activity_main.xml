<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Poincares SDK Demo"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.065" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Server Task"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="0.071" />

    <Button
        android:id="@+id/btnStartBySvConfig"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start by Server Config"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.035" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-black"
        android:text="Manual Task"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnStartBySvConfig"
        app:layout_constraintVertical_bias="0.108" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="156dp"
        android:layout_height="320dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="28dp"
        android:gravity="left"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@+id/linearLayout"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <TextView
            android:id="@+id/textView23"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:fontFamily="sans-serif-black"
            android:gravity="center"
            android:text="URL"
            android:textSize="16sp"
            android:typeface="monospace" />

        <EditText
            android:id="@+id/etPingUrl"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:ems="10"
            android:inputType="text"
            android:text="qq.com"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/etHttpUrl"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:ems="10"
            android:inputType="text"
            android:text="qq.com"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/etTcpPingUrl"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:ems="10"
            android:inputType="text"
            android:text="qq.com"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/etMtrUrl"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:ems="10"
            android:inputType="text"
            android:text="qq.com"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="52dp"
        android:layout_height="319dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="28dp"
        android:orientation="vertical"
        app:layout_constraintStart_toEndOf="@+id/linearLayout2"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <TextView
            android:id="@+id/textView31"
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:fontFamily="sans-serif-black"
            android:gravity="center"
            android:text="Count"
            android:textSize="16sp"
            android:typeface="monospace" />

        <EditText
            android:id="@+id/etPingCount"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:ems="10"
            android:inputType="text"
            android:text="-1"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/etHttpCount"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:ems="10"
            android:inputType="text"
            android:text="-1"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/etTcpPingCount"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:ems="10"
            android:inputType="text"
            android:text="-1"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <EditText
            android:id="@+id/etMtrCount"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:ems="10"
            android:inputType="text"
            android:text="-1"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="86dp"
        android:layout_height="319dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="28dp"
        android:gravity="right"
        android:orientation="vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <TextView
            android:id="@+id/textView10"
            android:layout_width="72dp"
            android:layout_height="36dp"
            android:fontFamily="sans-serif-black"
            android:gravity="center_horizontal|center_vertical"
            android:text="Type"
            android:textSize="16sp"
            android:typeface="serif"
            android:visibility="visible" />

        <TextView
            android:id="@+id/textView13"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:gravity="right|center_vertical"
            android:text="Ping:"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:gravity="right"
            android:text="empty"
            android:textSize="12sp"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:gravity="right|center_vertical"
            android:text="HTTP:"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:gravity="right"
            android:text="empty"
            android:textSize="12sp"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textView19"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:gravity="right|center_vertical"
            android:text="TCP Ping:"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="22dp"
            android:gravity="right"
            android:text="empty"
            android:textSize="12sp"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:gravity="right|center_vertical"
            android:text="MTR:"
            android:textSize="18sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="77dp"
        android:layout_height="319dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="28dp"
        android:gravity="left"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.34"
        app:layout_constraintStart_toEndOf="@+id/linearLayout3"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <TextView
            android:id="@+id/textView39"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <Button
            android:id="@+id/btnAddPing"
            android:layout_width="67dp"
            android:layout_height="34dp"
            android:paddingLeft="12dp"
            android:paddingTop="3dp"
            android:paddingRight="12dp"
            android:paddingBottom="3dp"
            android:text="Add" />

        <TextView
            android:id="@+id/textView33"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <Button
            android:id="@+id/btnAddHttp"
            android:layout_width="67dp"
            android:layout_height="34dp"
            android:paddingLeft="12dp"
            android:paddingTop="3dp"
            android:paddingRight="12dp"
            android:paddingBottom="3dp"
            android:text="Add" />

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <Button
            android:id="@+id/btnAddTcpPing"
            android:layout_width="67dp"
            android:layout_height="34dp"
            android:paddingLeft="12dp"
            android:paddingTop="3dp"
            android:paddingRight="12dp"
            android:paddingBottom="3dp"
            android:text="Add" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="36dp"
            android:gravity="right"
            android:text="blank"
            android:textSize="16sp"
            android:visibility="invisible" />

        <Button
            android:id="@+id/btnAddMtr"
            android:layout_width="67dp"
            android:layout_height="34dp"
            android:paddingLeft="12dp"
            android:paddingTop="3dp"
            android:paddingRight="12dp"
            android:paddingBottom="3dp"
            android:text="Add" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/copyrights"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="0.972" />

</androidx.constraintlayout.widget.ConstraintLayout>